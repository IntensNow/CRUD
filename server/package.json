{
  "name": "microwaves-crud",
  "version": "1.0.0",
  "description": "CRUD with Express, React, Jest",
  "main": "index.js",
  "scripts": {
    "start": "node dist/app.js",
    "server": "nodemon -r tsconfig-paths/register src/app.ts",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "build": "tsc -p .",
    "test": "jest"
  },
  "author": "Daniil Korobskoy",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "concurrently": "^6.3.0",
    "jest": "^27.2.4",
    "nodemon": "^2.0.13",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
}
